package br.com.fiap.checkpoint.model;

import java.util.Scanner;

public class Funcionario {
	//ATRIBUTOS
	private String nome, cpf, dataNasc, cargo, sexo, setor;
	private int idFunc;
	
	//CONSTRUTOR
	public Funcionario(int idFunc, String nome, String cpf,String dataNasc, 
						String sexo, String cargo, String setor) {
		
		this.idFunc = idFunc;
		this.nome = nome;
		this.cpf = cpf;
		this.dataNasc = dataNasc;
		this.sexo = sexo;
		this.cargo = cargo;
		this.setor = setor;
		
	}
	
	//METODOS
	public void incluirChamado(){
		System.out.println("Favor digitar o funcionário responsável pelo chamado:");
		nome = Scanner.next();
		
	}
	
	
	
	
}
